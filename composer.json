{
    "name": "sg/datatablesbundle",
    "type": "symfony-bundle",
    "description": "This Bundle integrates the jQuery DataTables plugin into your Symfony2 application. Compatible with Doctrine ORM.",
    "keywords": ["symfony2", "datatable", "grid", "table", "datagrid", "pagination"],
    "homepage": "https://github.com/stwe/DatatablesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "stwe"
        }
    ],
    "support": {
        "issues": "https://github.com/stwe/DatatablesBundle/issues"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "datatables/datatables",
                "version": "1.9.4",
                "dist": {
                    "type": "zip",
                    "url": "http://www.datatables.net/releases/DataTables-1.9.4.zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "datatables/datatables": "1.9.4",
        "twitter/bootstrap": "v2.3.2",
        "friendsofsymfony/jsrouting-bundle": "1.2.0"
    },
    "autoload": {
        "psr-0": { "Sg\\DatatablesBundle": "" }
    },
    "target-dir": "Sg/DatatablesBundle"
}

