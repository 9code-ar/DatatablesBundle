{% extends 'SgDatatablesBundle:Column:column.html.twig' %}

{% block data %}
    "mData": "{{ column.mData }}",
    "mRender": function(data, type, full) {
        return {{ column.mRender }}(data, type, full, "{{ column.trueIcon }}", "{{ column.falseIcon }}", "{{ column.trueLabel }}", "{{ column.falseLabel }}");
    },
{% endblock %}
